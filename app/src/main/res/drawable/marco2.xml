<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-4dp"
        android:left="-4dp"
        android:right="1dp"
        android:top="-4dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="@color/cafe2" />

            <solid android:color="#00FFFFFF" />

            <padding
                android:left="3dp"
                android:right="3dp"
                android:top="5dp"
                android:bottom="5dp" />
        </shape>
    </item>
</layer-list>
